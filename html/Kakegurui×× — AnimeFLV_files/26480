(function(sublime){'use strict';sublime.parseScript={};sublime.parseScript.response={"OK":"OK","EMPTY_RESPONSE":"EMPTY_RESPONSE","PARSE_ERROR":"PARSE_ERROR","PARSE_TIMEOUT":"PARSE_TIMEOUT","UNKNOWN_ERROR":"UNKNOWN_ERROR"};sublime.parseScript.query=function(url,options){var defaults={async:true,timeout:2500,onTimeout:sublime.noOp,success:sublime.noOp,error:sublime.noOp};options=sublime.merge(defaults,options);sublime.xhr(url,options);};sublime.parseScript.write=function(html,options){var defaults={responseReader:sublime.parseScript.readResponseFromIframe,errorCallback:sublime.noOp,successCallback:sublime.noOp,tryIntervalMs:25,tryRepeat:80};if(!options||!sublime.isObject(options)){options={};}
